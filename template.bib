@article{batista_assessing_2021,
  title = {Assessing Vegetation Heritage Value: The Alentejo Central (Portugal) as a Case Study},
  volume = {10},
  rights = {https://creativecommons.org/licenses/by/4.0/},
  issn = {2073-445X},
  url = {https://www.mdpi.com/2073-445X/10/3/307},
  doi = {10.3390/land10030307},
  shorttitle = {Assessing Vegetation Heritage Value},
  abstract = {Vegetation natural heritage can be valued in itself, but also as a habitat for many wildlife species. This study presents a methodological essay concerning the evaluation of plant heritage, applied in Central Alentejo, in the south of Portugal. This evaluation was based on the following criteria: horizontal and vertical structure of vegetation, tree cover density, rare plant species richness, phytocenotic maturity, importance as ecological corridor, historical record, scientific and educational potential and recreational potential. The vegetation heritage value ({VHV}) was calculated using a linear combination of the weighted criteria. In this way it was possible to identify the most interesting areas for conservation purposes. Of the 7500 km2 analyzed, 52\% are natural and seminatural areas, and of these, 18.8\% have very high {VHV}, 70.7\% have high {VHV} and only 10.5\% average {VHV}. The highest {VHV} areas are actually included under the protected areas of Natura 2000 sites.},
  pages = {307},
  number = {3},
  journaltitle = {Land},
  shortjournal = {Land},
  author = {Batista, Teresa and De Mascarenhas, José Manuel and Mendes, Paula and Pinto-Gomes, Carlos},
  urldate = {2024-04-22},
  date = {2021-03-17},
  langid = {english}
}

