\newcommand{\istmakechangelog} {%
  \InputIfFileExists {\jobname.chl}{}{%
    \ClassWarning{h2020proposal}{Change log not complete yet. Recompile to include all data.}}%
}%

\newcommand{\writechlog}{
  % create chlog table on auxiliary lwp file
  \setcounter{@row}{0} \newwrite\chlogfile
  \immediate\openout\chlogfile=\jobname.chl
  \immediate\write\chlogfile{%
  {\noexpand\fontsize{12}{15}\noexpand\color{INT-ACTorange}\noexpand\bf Document history}\noexpand\\[1ex] 
  \noexpand\setlength{\noexpand\parindent}{0pt}
  \noexpand\def\noexpand\arraystretch{1}
  \noexpand\arrayrulecolor{INT-ACTorange}
  \noexpand\def\noexpand\clinecolor{INT-ACTorange}
  \noexpand\fontsize{9}{12}
  \noexpand\begin{tabular}[t]{!{\noexpand\color{\noexpand\clinecolor}\noexpand\vrule}p{.12\noexpand\linewidth}!{\noexpand\color{\noexpand\clinecolor}\noexpand\vrule}p{.11\noexpand\linewidth}!{\noexpand\color{\noexpand\clinecolor}\noexpand\vrule}p{.27\noexpand\linewidth}!{\noexpand\color{\noexpand\clinecolor}\noexpand\vrule}p{.385\noexpand\linewidth}!{\noexpand\color{\noexpand\clinecolor}\noexpand\vrule}}
  \noexpand\arrayrulecolor{INT-ACTorange}\noexpand\hline
             \noexpand\rowcolor{INT-ACTorange}
             \noexpand\multicolumn{1}{l!{\noexpand\color{white}\noexpand\vline}}{\noexpand\cellcolor{INT-ACTorange}
             \noexpand\color{white}
             \noexpand\raisebox{-1.2ex}{Version No.}} &
             \noexpand\multicolumn{1}{l!{\noexpand\color{white}\noexpand\vline}}{\noexpand\cellcolor{INT-ACTorange}
             \noexpand\color{white}
             \noexpand\raisebox{-1.2ex}{Date}} &
\noexpand\multicolumn{1}{l!{\noexpand\color{white}\noexpand\vline}}{\noexpand\cellcolor{INT-ACTorange}
             \noexpand\color{white}
             \noexpand\raisebox{-1.2ex}{Contributors}} &
\noexpand\multicolumn{1}{l!{\noexpand\color{INT-ACTorange}\noexpand\vline}}{\noexpand\cellcolor{INT-ACTorange}
             \noexpand\color{white}
             \noexpand\raisebox{-1.2ex}{Notes}
                                                 \noexpand\color{INT-ACTorange}}\noexpand\\[1ex] \noexpand\hline}
  \whiledo{\value{@row}<\value{@changecount}}{%
             \stepcounter{@row}
             \ifodd\value{@row}\def\shadeofgray{white}\else\def\shadeofgray{white}\fi
             \immediate\write\chlogfile{%
             \noexpand\small\csname @changeversion\arabic{@row}\endcsname &
             \noexpand\small\csname @changedate\arabic{@row}\endcsname &                        
             \noexpand\small\csname @changeauthor\arabic{@row}\endcsname &
             \noexpand\small\csname @changesummary\arabic{@row}\endcsname \noexpand\\\noexpand\arrayrulecolor{INT-ACTorange}\noexpand\hline
             }
   }
 \immediate\write\chlogfile{  \noexpand\arrayrulecolor{INT-ACTorange}\noexpand\hline\noexpand\end{tabular}}
 \closeout\chlogfile
}
%\multicolumn{1}{l!{\color{white}\vline}}{\cellcolor{INT-ACTorange}\color{white}
\AtEndDocument{%
 \writechlog 
  }

  
